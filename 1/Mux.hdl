/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=sel, out=notsel);
    Nand(a=a, b=notsel, out=x);
    Nand(a=sel, b=b, out=y);
    Nand(a=x, b=y, out=out);
}
